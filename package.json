    }
  },
  "lint-staged": {
    "(!src/util/graphql/generated/)*.{ts,tsx,js,jsx}": [
      "eslint --max-warnings 0 --fix"
    ],
    "*.{ts,tsx,json,md,scss,yaml,yml}": [
