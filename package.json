{
  "name": "answer-bot-flow-composer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf ./public/dist node_modules coverage && find i18n ! \\( -name 'en-us.yml' -o -name 'locales.json' \\) -delete",
    "start:local": "START_ENV=local ./bin/start.sh",
    "start:zdi-backend": "START_ENV=zdi-backend ./bin/start.sh",
    "start:zdi": "START_ENV=zdi ./bin/start.sh",
    "build:dev": "BUILD_ENV=development ./bin/build.sh",
    "build": "BUILD_ENV=production ./bin/build.sh",
    "fetch-locales": "./node_modules/@zendesk/client-i18n-tools/bin/download-locales --cldr-languages --destination='./i18n' --packages='flow_builder' --locales='./i18n/locales.json'",
    "generateScreenshot": "node node_modules/zendesk-i18n-image-generator/dist/generateScreenshot.js",
    "lint": "eslint --max-warnings 0 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint --max-warnings 0 --fix 'src/**/*.{ts,tsx}'",
    "rm-sourcemaps": "./bin/rm-sourcemaps.sh",
    "test": "jest --watch --no-coverage",
    "test:coverage": "CI=1 yarn jest --maxWorkers=4 --coverageReporters html lcov text-summary --verbose",
    "test:ci": "yarn jest --coverageReporters lcov text-summary --verbose",
    "type-check": "tsc --noEmit",
    "codegen": "./bin/codegen.sh",
    "prettier": "prettier --check 'src/**/*.{ts,tsx}'",
    "prettier:fix": "prettier --write 'src/**/*.{ts,tsx}'",
    "deadcode": "ts-prune"
  },
  "dependencies": {
